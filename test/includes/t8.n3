# Test the log:includes function

@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix s: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix daml:  <http://www.daml.org/2000/10/daml-ont#> .
@prefix dpo:  <http://www.daml.org/2001/03/daml+oil#> .
@prefix ex:   <http://www.daml.org/2000/10/daml-ex#> .
@prefix log:  <http://www.w3.org/2000/10/swap/log#> .

@prefix : <#>.  # Local stuff

# Usage:   cwm includes.n3 -think
#
# Output should conclude all test* a :success and no failures.
#

{  "abc" log:greaterThan "aaa" } log:implies { :testh1 a :success }.
{  "abc" log:greaterThan "abc" } log:implies { :testh_1 a :FAILURE }.
{  "aaa" log:greaterThan "abc" } log:implies { :testh_2 a :FAILURE }.

{  "abc" log:notGreaterThan "aaa" } log:implies { :testh_3 a :FAILURE }.
{  "abc" log:notGreaterThan "abc" } log:implies { :testh2 a :success }.
{  "aaa" log:notGreaterThan "abc" } log:implies { :testh3 a :success }.


{  "abc" log:lessThan "aaa" } log:implies { :testh_4 a :FAILURE }.
{  "abc" log:lessThan "abc" } log:implies { :testh_5 a :FAILURE }.
{  "aaa" log:lessThan "abc" } log:implies { :testh4 a :success }.

{  "abc" log:notLessThan "aaa" } log:implies { :testh5 a :success }.
{  "abc" log:notLessThan "abc" } log:implies { :testh6 a :success }.
{  "aaa" log:notLessThan "abc" } log:implies { :testh_6 a :FAILURE }.

{ "abc" log:startsWith "a" } log:implies { :testh7 a :success }.
{ "def" log:startsWith "def" } log:implies { :testh8 a :success }.
{ "a" log:startsWith "abc" } log:implies { :testh_7 a :FAILURE }.

# ends


