     @prefix : <http://www.w3.org/2000/10/swap/log#> .
     @prefix daml: <http://www.daml.org/2001/03/daml+oil#> .
     @prefix rules: <#> .
    
    this     :forAll rules:p,
                rules:x,
                rules:y;
         :forSome {rules:p     a daml:TransitiveProperty .
        },
                {{
            {
                rules:x     rules:p rules:y .
                rules:y     rules:p rules:z .
                
                }     :implies {rules:x     rules:p rules:z .
                } .
            
            }     a :Truth;
             :forAll rules:x,
                    rules:y,
                    rules:z .
        } .
    
    rules:_g0 :- {
        rules:p     a daml:TransitiveProperty .
        
        }     :implies {{
            {
                rules:x     rules:p rules:y .
                rules:y     rules:p rules:z .
                
                }     :implies {rules:x     rules:p rules:z .
                } .
            
            }     a :Truth;
             :forAll rules:x,
                    rules:y,
                    rules:z .
        };
         :means {{
            {
                rules:x     rules:p rules:y .
                rules:y     rules:p rules:z .
                
                }     :implies {rules:x     rules:p rules:z .
                } .
            
            }     a :Truth;
             :forAll rules:x,
                    rules:y,
                    rules:z .
        } .
    
    rules:_g1 :- {
        {
            {
                rules:x     rules:p rules:y .
                rules:y     rules:p rules:z .
                
                }     :implies {rules:x     rules:p rules:z .
                } .
            
            }     a :Truth;
             :forAll rules:x,
                    rules:y,
                    rules:z .
        
        }     :implies {rules:p     a daml:TransitiveProperty .
        };
         :means {rules:p     a daml:TransitiveProperty .
        } .
    {
        rules:x     :means rules:y .
        
        }     :implies {rules:x     :implies rules:y .
        rules:y     :implies rules:x .
        } .
    {
        rules:x     :implies rules:y .
        rules:y     :implies rules:x .
        
        }     :implies {rules:x     :means rules:y .
        } .
    {
        {
            rules:_gs2     rules:ancestor rules:_gs3 .
            rules:_gs3     rules:ancestor rules:_gs4 .
            
            }     :implies {rules:_gs2     rules:ancestor rules:_gs4 .
            } .
        
        }     a :Truth;
         :forAll rules:_gs2,
                rules:_gs3,
                rules:_gs4 .
    
    rules:ancestor     a daml:TransitiveProperty .
    
    rules:granpa     rules:ancestor rules:bill,
                rules:pa .
    
    rules:pa     rules:ancestor rules:bill .
    
