     @prefix daml: <http://www.daml.org/2001/03/daml+oil#> .
     @prefix ex: <daml-ex.n3#> .
     @prefix log: <http://www.w3.org/2000/10/swap/log#> .
     @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
     @prefix rules: <rules12.n3#> .
    
     @forSome <#_g0>,
                <#_g1>,
                <#_g2>,
                <run#li5>,
                <run#li4>,
                <run#li7>,
                <run#li6>,
                <run#li3>,
                <run#li2>,
                <run#li1> .
    
    <run#li5>     rdf:first ex:Man;
         rdf:rest <run#li4> .
    
    <run#li4>     rdf:first ex:Woman;
         rdf:rest () .
    
    <run#li7>     rdf:first ex:TallThing;
         rdf:rest <run#li6> .
    
    <run#li6>     rdf:first ex:Man;
         rdf:rest () .
    
    <run#li3>     rdf:first ex:short;
         rdf:rest <run#li2> .
    
    <run#li2>     rdf:first ex:medium;
         rdf:rest <run#li1> .
    
    <run#li1>     rdf:first ex:tall;
         rdf:rest () .
    
    ex:Male     a daml:Class .
    
    ex:occupation     a daml:Property .
    
    <daml-ex.n3>     daml:imports <http://www.daml.org/2000/10/daml-ont> .
    
    ex:height     daml:domain ex:Person .
    
    rules:ancestor     a daml:TransitiveProperty .
    
    ex:height     daml:range ex:Height .
    
    ex:Person     daml:comment "every person is a man or a woman" .
    
    ex:mom     a daml:Property .
    
    ex:Car     daml:comment "no car is a person" .
    
    ex:mom     = ex:mother .
    
    rules:pa     rules:ancestor rules:bill .
    
    ex:Height     a daml:Class .
    
    ex:Person     daml:disjointUnionOf <run#li5> .
    
    ex:mother     daml:subProperty ex:parent .
    
    ex:Person     daml:restrictedBy <#_g1> .
    
    ex:Woman     daml:subClassOf ex:Person .
    
    ex:Adam     a daml:Person .
    
    <#_g1>     a daml:Restriction .
    
    ex:parent     daml:domain ex:Animal .
    
    ex:Adam     daml:label "Adam" .
    
    <daml-ex.n3>     daml:comment "An example ontology";
         daml:versionInfo "" .
    
    ex:TallMan     daml:intersectionOf <run#li7> .
    
    ex:TallThing     a daml:Class .
    
    <#_g2>     a daml:Restriction;
         daml:toValue ex:tall .
    
    <daml-ex.n3>     a daml:Ontology .
    
    ex:Woman     daml:subClassOf ex:Female .
    
    ex:Man     a daml:Class .
    
    ex:Female     daml:subClassOf ex:Animal .
    
    ex:child     a daml:Property .
    
    ex:Man     daml:subClassOf ex:Person .
    
    ex:descendant     a daml:TransitiveProperty .
    
    ex:Male     daml:subClassOf ex:Animal .
    
    ex:father     a daml:Property .
    
    ex:Height     daml:oneOf <run#li3> .
    
    ex:father     daml:range ex:Man;
         daml:subProperty ex:parent .
    
    ex:height     a daml:Property .
    
    ex:Person     a daml:Class .
    
    ex:parent     daml:cardinality "2" .
    
    <#_g0>     a daml:Class .
    
    ex:Car     daml:subClassOf <#_g0> .
    
    ex:mother     daml:range ex:Woman .
    
    ex:Female     a daml:Class .
    
    ex:Person     daml:subClassOf ex:Animal .
    
    ex:occupation     daml:maxCardinality "1" .
    
    <#_g1>     daml:onProperty ex:parent .
    
    ex:parent     a daml:Property .
    
    <#_g1>     daml:toClass ex:Person .
    
    ex:Adam     daml:comment "Adam is a person." .
    
    ex:TallMan     a daml:Class .
    
    ex:Animal     a daml:Class;
         daml:comment """This class of animals is illustrative of a number of
	ontological idioms.""" .
    
    ex:Woman     a daml:Class .
    
    ex:Animal     daml:label "Animal" .
    
    rules:granpa     rules:ancestor rules:pa .
    
    ex:TallThing     daml:restrictedBy <#_g2> .
    
    <#_g2>     daml:onProperty ex:height .
    
    <#_g0>     daml:complementOf ex:Person .
    
    ex:Car     a daml:Class .
    
    ex:mother     a daml:UniqueProperty .
    
    ex:Female     daml:disjointFrom ex:Male .
    
    ex:ancestor     a daml:TransitiveProperty;
         daml:label "ancestor" .
    
    ex:child     daml:inverseOf ex:parent .
    
    ex:Man     daml:subClassOf ex:Male .
    
    ex:father     daml:cardinality "1" .
    
