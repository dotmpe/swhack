     @prefix ex: <http://www.daml.org/2001/03/daml-ex#> .
     @prefix : <http://www.daml.org/2001/03/daml+oil#> .
     @prefix exg1: <#> .
     @prefix daml: <http://www.daml.org/2001/03/daml-ont#> .
     @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
     @prefix s: <http://www.w3.org/2000/01/rdf-schema#> .
     @prefix log: <http://www.w3.org/2000/10/swap/log#> .
    
    <daml-ex.rdf#Adam>     a :Person;
         :comment "Adam is a person.";
         :label "Adam" .
    
    <daml-ex.rdf#Animal>     a :Class;
         :comment """This class of animals is illustrative of a number of
	ontological idioms.""";
         :label "Animal" .
    
    <daml-ex.rdf#Car>     a :Class;
         :comment "no car is a person";
         :subClassOf  [
             a :Class;
             :complementOf <daml-ex.rdf#Person> ] .
    
    <daml-ex.rdf#Female>     a :Class;
         :disjointFrom <daml-ex.rdf#Male>;
         :subClassOf <daml-ex.rdf#Animal> .
    
    <daml-ex.rdf#Height>     a :Class;
         :oneOf  (
        <daml-ex.rdf#short> 
        <daml-ex.rdf#medium> 
        <daml-ex.rdf#tall>  ) .
    
    <daml-ex.rdf#Male>     a :Class;
         :subClassOf <daml-ex.rdf#Animal> .
    
    <daml-ex.rdf#Man>     a :Class;
         :subClassOf <daml-ex.rdf#Male>,
                <daml-ex.rdf#Person> .
    
    <daml-ex.rdf#Person>     a :Class;
         :comment "every person is a man or a woman";
         :disjointUnionOf  (
        <daml-ex.rdf#Man> 
        <daml-ex.rdf#Woman>  );
         :restrictedBy  [
             a :Restriction;
             :onProperty <daml-ex.rdf#parent>;
             :toClass <daml-ex.rdf#Person> ];
         :subClassOf <daml-ex.rdf#Animal> .
    
    <daml-ex.rdf#TallMan>     a :Class;
         :intersectionOf  (
        <daml-ex.rdf#TallThing> 
        <daml-ex.rdf#Man>  ) .
    
    <daml-ex.rdf#TallThing>     a :Class;
         :restrictedBy  [
             a :Restriction;
             :onProperty <daml-ex.rdf#height>;
             :toValue <daml-ex.rdf#tall> ] .
    
    <daml-ex.rdf#Woman>     a :Class;
         :subClassOf <daml-ex.rdf#Female>,
                <daml-ex.rdf#Person> .
    
    <daml-ex.rdf#ancestor>     a :TransitiveProperty;
         :label "ancestor" .
    
    <daml-ex.rdf#child>     a :Property;
         :inverseOf <daml-ex.rdf#parent> .
    
    <daml-ex.rdf#descendant>     a :TransitiveProperty .
    
    <daml-ex.rdf#father>     a :Property;
         :cardinality "1";
         :range <daml-ex.rdf#Man>;
         :subProperty <daml-ex.rdf#parent> .
    
    <daml-ex.rdf#height>     a :Property;
         :domain <daml-ex.rdf#Person>;
         :range <daml-ex.rdf#Height> .
    
    <daml-ex.rdf#mom>     a :Property;
         = <daml-ex.rdf#mother> .
    
    <daml-ex.rdf#mother>     a :UniqueProperty;
         :range <daml-ex.rdf#Woman>;
         :subProperty <daml-ex.rdf#parent> .
    
    <daml-ex.rdf#occupation>     a :Property;
         :maxCardinality "1" .
    
    <daml-ex.rdf#parent>     a :Property;
         :cardinality "2";
         :domain <daml-ex.rdf#Animal> .
    
    <daml-ex.rdf>     a :Ontology;
         :comment "An example ontology";
         :imports <http://www.daml.org/2000/10/daml-ont>;
         :versionInfo "" .
    
