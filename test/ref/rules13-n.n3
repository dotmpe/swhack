     @prefix daml: <http://www.daml.org/2001/03/daml+oil#> .
     @prefix : <#> .
     @prefix log: <http://www.w3.org/2000/10/swap/log#> .
    
    this     log:forAll :p,
                :x,
                :y;
         log:forSome {:p     a daml:TransitiveProperty .
        },
                {{
            {
                :x     :p :y .
                :y     :p :z .
                
                }     log:implies {:x     :p :z .
                } .
            
            }     a log:Truth;
             log:forAll :x,
                    :y,
                    :z .
        } .
    
    :_g0 :- {
        :p     a daml:TransitiveProperty .
        
        }     log:implies {{
            {
                :x     :p :y .
                :y     :p :z .
                
                }     log:implies {:x     :p :z .
                } .
            
            }     a log:Truth;
             log:forAll :x,
                    :y,
                    :z .
        };
         log:means {{
            {
                :x     :p :y .
                :y     :p :z .
                
                }     log:implies {:x     :p :z .
                } .
            
            }     a log:Truth;
             log:forAll :x,
                    :y,
                    :z .
        } .
    
    :_g1 :- {
        {
            {
                :x     :p :y .
                :y     :p :z .
                
                }     log:implies {:x     :p :z .
                } .
            
            }     a log:Truth;
             log:forAll :x,
                    :y,
                    :z .
        
        }     log:implies {:p     a daml:TransitiveProperty .
        };
         log:means {:p     a daml:TransitiveProperty .
        } .
    {
        :x     log:means :y .
        
        }     log:implies {:x     log:implies :y .
        :y     log:implies :x .
        } .
    {
        :x     log:implies :y .
        :y     log:implies :x .
        
        }     log:implies {:x     log:means :y .
        } .
    {
        {
            :_gs2     :ancestor :_gs3 .
            :_gs3     :ancestor :_gs4 .
            
            }     log:implies {:_gs2     :ancestor :_gs4 .
            } .
        
        }     a log:Truth;
         log:forAll :_gs2,
                :_gs3,
                :_gs4 .
    
    :ancestor     a daml:TransitiveProperty .
    
    :granpa     :ancestor :bill,
                :pa .
    
    :pa     :ancestor :bill .
    
