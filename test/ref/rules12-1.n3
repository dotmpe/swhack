     @prefix : <#> .
     @prefix daml: <http://www.daml.org/2001/03/daml+oil#> .
     @prefix log: <http://www.w3.org/2000/10/swap/log#> .
    
    this     log:forAll :_gs1,
                :_gs2,
                :_gs3,
                :p .
    {
        :p     a daml:TransitiveProperty .
        
        }     log:implies {this     log:forAll :x,
                    :y,
                    :z .
        {
            :x     :p :y .
            :y     :p :z .
            
            }     log:implies {:x     :p :z .
            } .
        } .
    {
        :_gs1     :ancestor :_gs3 .
        :_gs3     :ancestor :_gs2 .
        
        }     log:implies {:_gs1     :ancestor :_gs2 .
        } .
    
    :ancestor     a daml:TransitiveProperty .
    
    :granpa     :ancestor :bill,
                :pa .
    
    :pa     :ancestor :bill .
    
