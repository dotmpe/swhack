     @prefix : <#> .
     @prefix daml: <http://www.daml.org/2001/03/daml+oil#> .
     @prefix log: <http://www.w3.org/2000/10/swap/log#> .
    
    this     log:forAll :_gs3,
                :_gs4,
                :_gs5,
                :p .
    {
        :p     a daml:TransitiveProperty .
        
        }     log:implies {this     log:forAll :x,
                    :y,
                    :z .
        {
            :x     :p :y .
            :y     :p :z .
            
            }     log:implies {:x     :p :z .
            } .
        } .
    {
        :_gs3     :ancestor :_gs4 .
        :_gs4     :ancestor :_gs5 .
        
        }     log:implies {:_gs3     :ancestor :_gs5 .
        } .
    
    :ancestor     a daml:TransitiveProperty .
    
    :granpa     :ancestor :pa .
    
    :pa     :ancestor :bill .
    
