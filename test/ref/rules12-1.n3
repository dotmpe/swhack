









     @prefix log: <http://www.w3.org/2000/10/swap/log.n3#> .
     @prefix daml: <http://www.daml.org/2000/10/daml-ont#> .
     @prefix : <rules12.n3#> .
    
    <>     log:forAll :p .
    {
        {
            :x     :ancestor :y .
            :y     :ancestor :z .
            
            }     log:implies {:x     :ancestor :z .
            } .
        
        }     a log:Truth;
         log:forAll :x,
                :y,
                :z .
    {
        :p     a daml:TransitiveProperty .
        
        }     log:implies {{
            {
                :x     :p :y .
                :y     :p :z .
                
                }     log:implies {:x     :p :z .
                } .
            
            }     a log:Truth;
             log:forAll :x,
                    :y,
                    :z .
        } .
    
    :ancestor     a daml:TransitiveProperty .
    
    :granpa     :ancestor :pa,
                :bill .
    
    :pa     :ancestor :bill .
    
 #ENDS
