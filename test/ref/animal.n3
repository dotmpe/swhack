     @prefix : <#> .
     @prefix daml: <http://www.daml.org/2000/12/daml+oil#> .
     @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
     @prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
    
    :Animal     a rdfs:Class;
         daml:restrictedBy  [
             a daml:Restriction;
             daml:onProperty :parent;
             daml:cardinality "2" ];
         rdfs:label "Animal" .
    
